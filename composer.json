{
    "name": "pathfindermediagroup/discord-slash",
    "description": "INTERNAL package for creating Discord bots",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PathfinderMediaGroup\\DiscordSlash\\": "src/"
        }
    },
    "require": {
        "php": "^8.4",
        "ext-gmp": "*",
        "ext-json": "*",
        "discord/interactions": "^2.2",
        "simplito/elliptic-php": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13"
    },
    "scripts": {
        "cgl": [
            "vendor/bin/php-cs-fixer fix --config .php-cs-fixer.php"
        ]
    }
}
